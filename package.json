{
  "name": "palapala",
  "version": "0.0.1",
  "description": "Collaborative whiteboard web app",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify js/client-db.js -o js/browser.js",
    "start": "nodemon -i lib/ -i node_modules/ js/server.js",
    "dist": "mkdir -p static && cp lib/spectrum-colorpicker/spectrum.css lib/jquery/dist/jquery.slim.min.js lib/spectrum-colorpicker/spectrum.js lib/popper.js/dist/umd/popper.min.js lib/bootstrap/dist/js/bootstrap.min.js  lib/bootstrap/dist/css/bootstrap.min.css lib/acorn/dist/acorn.js lib/paper/dist/paper-full.js static/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grady/palapala.git"
  },
  "keywords": [
    "whiteboard"
  ],
  "author": "Grady Weyenberg",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/grady/palapala/issues"
  },
  "homepage": "https://github.com/grady/palapala#readme",
  "dependencies": {
    "acorn": "^7.1.1",
    "bootstrap": "^4.4.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jquery": "^3.5.1",
    "lodash.isequal": "^4.5.0",
    "paper": "^0.12.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "popper.js": "^1.16.0",
    "reconnecting-websocket": "^4.4.0",
    "sharedb": "^1.3.3",
    "sharedb-mongo": "^1.0.0-beta.13",
    "shortid": "^2.2.15",
    "simplify-js": "^1.2.4",
    "spectrum-colorpicker": "^1.8.0",
    "websocket-json-stream": "0.0.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "nodemon": "^2.0.3"
  }
}
